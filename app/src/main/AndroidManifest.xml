<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.test.myapplication">

    <application
        android:name="com.example.global.AppApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:allowBackup"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity android:name="com.example.test.databinding.DataBindingActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:screenOrientation="sensor"/>
        <activity android:name="com.example.test.aac.FragmentBindingActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:screenOrientation="sensor"/>
        <activity android:name="com.example.test.aac.AddUserProfileActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:screenOrientation="sensor"/>
        <activity android:name="com.example.test.aac.UserProfileActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:screenOrientation="sensor"/>
        <activity android:name="com.example.test.aac.EditUserProfileActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"/>
        <activity
            android:name="com.example.activity.NestedScrollViewActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
        <activity
            android:name="com.example.activity.FoldListActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:screenOrientation="sensor" />
        <activity
            android:name="com.example.activity.ScrollingViewBehaviorActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:screenOrientation="sensor" />
        <activity
            android:name="com.example.activity.CollapsingToolbarLayoutActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
        <activity
            android:name="com.example.activity.InfinityListActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
        <activity
            android:name="com.example.test.material.BottomNavigationActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
        <activity
            android:name="com.example.activity.CardScrollActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar"
            android:screenOrientation="sensor" />
        <activity
            android:name="com.example.activity.MaterialButtonActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
        <activity
            android:name="com.example.activity.PagerActivity"
            android:theme="@style/Theme.MaterialComponents.Light.NoActionBar" />
        <activity android:name="com.example.activity.UIListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FourActivity" />
        <activity
            android:name=".ThirdActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".SecondActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="exampleapp" />
            </intent-filter>
        </activity>
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>